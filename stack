import java.util.*;
  
public class StackDemo {
    public static void main(String args[])
    {
        Stack<Integer> stack = new Stack<Integer>();
        stack.add(10);
        stack.add(15);
        stack.add(30);
        stack.add(20);
        stack.add(5);
 
        System.out.println("Stack: " + stack);
  
        System.out.println("The size is: " + stack.size());
    }
}
